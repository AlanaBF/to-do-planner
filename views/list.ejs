<%- include("header") -%>
<%- include("navbar") -%>
<div class="min-height-todo">
<div class="box" id="heading">
    <h1><%= listTitle %></h1>
</div>

<div class="box">
    <% for(let i=0; i<newListItems.length; i++) { %>
        <div class="item"> 
            <input type ="checkbox">  
            <p><%= newListItems[i] %> </p>
        </div>
        <% } %>

<form class="item" action="/general" method="post">
    <input type="text" name="newItem" autocomplete="off" placeholder="add to do item" />
    <button name="list" type="submit" value=<%= listTitle %>>+</button>
</form>
</div>
</div>
<%- include("footer") -%>